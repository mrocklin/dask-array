<details class="dask-array-repr" style="font-family: -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, 'Helvetica Neue', Arial, sans-serif;">
    <summary style="cursor: pointer; list-style: none;">
        <table style="border-collapse: separate; border-spacing: 0; display: inline-table;">
            <tr>
                <td style="vertical-align: top;">
                    <table style="border-collapse: collapse; font-size: 14px;">
                        <thead>
                            <tr>
                                <td style="padding: 6px 12px;"></td>
                                <th style="padding: 6px 12px; text-align: right; font-weight: 400; color: #78716c;">Array</th>
                                <th style="padding: 6px 12px; text-align: right; font-weight: 400; color: #78716c;">Chunk</th>
                            </tr>
                        </thead>
                        <tbody>
                            {% if nbytes %}
                            <tr>
                                <th style="padding: 6px 12px; text-align: left; font-weight: 400; color: #a8a29e; border-top: 1px solid #e7e5e4;">Bytes</th>
                                <td style="padding: 6px 12px; text-align: right; font-weight: 600; color: #1c1917; border-top: 1px solid #e7e5e4;">{{ nbytes }}</td>
                                <td style="padding: 6px 12px; text-align: right; font-weight: 600; color: #1c1917; border-top: 1px solid #e7e5e4;">{{ cbytes }}</td>
                            </tr>
                            {% endif %}
                            <tr>
                                <th style="padding: 6px 12px; text-align: left; font-weight: 400; color: #a8a29e; border-top: 1px solid #e7e5e4;">Shape</th>
                                <td style="padding: 6px 12px; text-align: right; font-weight: 600; color: #1c1917; border-top: 1px solid #e7e5e4;">{{ array.shape }}</td>
                                <td style="padding: 6px 12px; text-align: right; font-weight: 600; color: #1c1917; border-top: 1px solid #e7e5e4;">{{ array.chunksize }}</td>
                            </tr>
                            <tr>
                                <th style="padding: 6px 12px; text-align: left; font-weight: 400; color: #a8a29e; border-top: 1px solid #e7e5e4;">Nodes</th>
                                <td style="padding: 6px 12px; text-align: right; font-weight: 600; color: #1c1917; border-top: 1px solid #e7e5e4;">{{ n_expr }}</td>
                                <td style="padding: 6px 12px; border-top: 1px solid #e7e5e4;"></td>
                            </tr>
                        </tbody>
                    </table>
                </td>
                <td style="vertical-align: middle; padding-left: 24px;">
                {{ grid }}
                </td>
            </tr>
        </table>
    </summary>
    <div style="padding: 12px 0;">{{ expr_flow }}</div>
</details>
